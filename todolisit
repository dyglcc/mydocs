5.插件增加渠道meta字段




delimiter $$
drop procedure if exists test_proc_paging; $$
CREATE PROCEDURE test_proc_paging(
IN pageNum int,
IN pageSize int
)
BEGIN
    SET @pageSize = pageSize;
    SET @pageNum = pageNum;
    PREPARE s1 FROM "
    select s.name as stu,t.name as teacher from student.stu_info s left join teacher.teacher_info t on s.teacher_id = t.id
    limit ?,?";
    EXECUTE s1 USING @pageNum,@pageSize;
    DEALLOCATE PREPARE s1;
    SELECT FOUND_ROWS() as recordCounts;
END




delimiter $$
drop procedure if exists test_proc_paging; $$
CREATE PROCEDURE test_proc_paging(
IN pageNum int,
IN pageSize int
)
BEGIN
    SET @pageSize = pageSize;
    SET @pageNum = pageNum;
    PREPARE s1 FROM "
    select sql_calc_found_rows * from teacher_info limit ?,?";
    EXECUTE s1 USING @pageNum,@pageSize;
    DEALLOCATE PREPARE s1;
	select found_rows() as count;
END


